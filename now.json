{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node-server@canary"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "IS_NOW": "true",
    "ESI_CLIENT": "@memberbot-esi-client",
    "ESI_SECRET": "@memberbot-esi-secret",
    "REDIRECT_URL": "https://member-bot.mckernanin.now.sh",
    "JWT_SECRET": "@memberbot-jwt-secret",
    "JWT_EXPIRE": "5d",
    "DB_AWS_ACCESS_KEY_ID": "@dynamo-aws-access-key",
    "DB_AWS_SECRET_ACCESS_KEY": "@dynamo-aws-secret-key",
    "DB_AWS_REGION": "us-west-2"
  }
}
